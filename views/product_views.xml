<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="product_product_form_dhz_dymo_button_core" model="ir.ui.view">
    <field name="name">product.product.form.dhz.dymo.button.core</field>
    <field name="model">product.product</field>
    <field name="inherit_id" ref="product.product_normal_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//header" position="inside">
        <button name="action_dhz_quick_print_label" type="object" string="Print la Dymo" class="oe_highlight"/>
      </xpath>
    </field>
  </record>

  <record id="product_template_form_dhz_dymo_button_core" model="ir.ui.view">
    <field name="name">product.template.form.dhz.dymo.button.core</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//header" position="inside">
        <button name="action_dhz_quick_print_label" type="object" string="Print la Dymo" class="oe_highlight"/>
      </xpath>
    </field>
  </record>
</odoo>
